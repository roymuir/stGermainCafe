{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.5.3",
		"silverstripe/framework": "3.5.3",
		"silverstripe/reports": "3.5.3",
		"silverstripe/siteconfig": "3.5.3",
		"silverstripe-themes/simple": "3.1.*",
		"unclecheese/betterbuttons": "^1.3",
		"silverstripe/userforms": "^4.3",
		"silverstripe-australia/gridfieldextensions": "^1.4",
		"gdmedia/ss-auto-git-ignore": "^1.0",
		"unclecheese/display-logic": "1.4.x-dev",
		"silverstripe/googlesitemaps": "1.x-dev"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"scripts": {
     	"post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
